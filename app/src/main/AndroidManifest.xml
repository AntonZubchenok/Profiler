<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wiseass.profiler">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".createaccount.CreateAccountActivity" />
        <activity android:name=".profilepage.ProfilePageActivity"/>
        <activity android:name=".photogallery.PhotoGalleryActivity" />
        <activity android:name=".photodetail.PhotoDetailActivity" />
        <activity android:name=".profiledetail.ProfileDetailActivity" />
        <activity android:name=".profilesettings.ProfileSettingsActivity"/>
    </application>

</manifest>